      },
      "package": "com.sbateman.zhongmap",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c6648226-8c45-45d9-a046-2efe0c94e261"
      }
    },
    "owner": "nowaymr"
  }
